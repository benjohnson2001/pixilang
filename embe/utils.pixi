include "../list.pixi"

fn in($channel_id) {
    $this = new()
    $this.channel_id = $channel_id

    $this.gen = { // $this, $channels, $frames, $in_channels
        $this = $1
        $channels = $2
        $frames = $3
        $in_channels = $4

        if $in_channels >= 0 {
            copy($channels[0], $in_channels[$this.channel_id])
            ret (1)
        } else {
            ret (0)
        }
    }

    ret ($this)
}
